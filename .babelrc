{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3.19"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [["@babel/plugin-proposal-class-properties", { "loose": false }]],
  "assumptions": {
    "setPublicClassFields": false
  }
}
